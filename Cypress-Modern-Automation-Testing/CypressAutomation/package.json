{
  "name": "cypressautomation",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "npx cypress run",
    "headed": "npx cypress run --headed",
    "chromeTest": "npx cypress run --browser chrome",
    "record": "npx cypress run --record --key a50f9f80-5977-4dc7-b464-a98975139cf9",
